{
  "extends": [
    "kentcdodds",
    "kentcdodds/react",
    "kentcdodds/jest",
    "kentcdodds/prettier",
  ],
  "rules": {
    "max-len": "off",
    "semi": "off",
    "comma-dangle": "off",
    "quote": "off",
    "max-lines": "off",
    "func-style": "off",
    "no-eq-null": "off",
    "eqeqeq": "off",
    "no-nested-ternary": "off"
  }
}
